@model List<HireUs>
<div class="container-fluid">
    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">
                <a href="/admin/HomeBanner/create">Create</a>
            </h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr>
                            <th>#Id</th>
                            <th>Email</th>
                            <th>Title</th>
                            <th>Check</th>
                            <th>Options</th>
                        </tr>
                    </thead>
                    <tfoot>
                        <tr>
                            <th>#Id</th>
                            <th>Email</th>
                            <th>Title</th>
                            <th>Check</th>
                            <th>Options</th>

                        </tr>
                    </tfoot>
                    <tbody>
                        @* *@
                        @foreach (var home in Model)
                        {
                            <tr>
                                <th>@home.Id</th>
                                <th>@home.Email</th>
                                <th>@home.Name</th>
                                <th>@home.Check</th>
                                <th>Options</th>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>